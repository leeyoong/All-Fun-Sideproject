<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dashtouch"
    android:layout_width="match_parent"
    android:layout_height="230dp"
    android:background="@drawable/custom_layout2"
    android:layout_gravity="center"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="92dp"
        android:background="@drawable/custom_layout4">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="340dp">

        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>

    <TextView
        android:id="@+id/dashtitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"

        android:layout_marginBottom="4dp"
        android:text="해커톤 ALL-Fun project"
        android:textColor="@color/milk"
        android:textSize="16dp" />

    <TextView
        android:id="@+id/dashtitle2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="30dp"
        android:text="해커톤 ALL-Fun project"
        android:textColor="@color/milk"
        android:textSize="12dp" />

    <LinearLayout
        android:layout_width="135dp"
        android:layout_height="45dp"
        android:layout_gravity="left
"
        android:layout_marginStart="8dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/dashalarm"
            android:layout_width="41dp"
            android:layout_height="match_parent"
            android:layout_marginStart="8dp"
            app:srcCompat="@drawable/ic_baseline_notifications_none_24" />

    </LinearLayout>
</LinearLayout>